{
    "editor.autoClosingOvertype": "always",
    "editor.minimap.enabled": true,
    "editor.lineNumbers": "relative",
    "breadcrumbs.enabled": true,
    "git.enableSmartCommit": true,
    "git.autofetch": true,
    "editor.cursorSurroundingLines": 10,
    "editor.wordWrap": "on",
    "terminal.integrated.fontFamily": "Fira Mono for Powerline",
    "editor.renderLineHighlight": "all",
    "workbench.colorCustomizations": {
    "editor.lineHighlightBackground": "#2e2c2e"
    },

    "vim.useSystemClipboard": true,
    "vim.handleKeys": {
        "<C-d>": true,
        "<C-a>": false,
        "<C-f>": true,
        "<C-h>": false,
        "<C-c>": false,
        "<C-w>": false,
        "<C-x>": false,
        "<C-n>": false,    
        },
    "vim.insertModeKeyBindings": [
        {
            "before": ["j", "k"],
            "after" : ["<Esc>"]
        }
    ],

    "matlab.mlintpath": "C:\\\\Program Files\\\\MATLAB\\\\R2019a\\\\bin\\\\win64\\\\mlint.exe",
    "matlab.matlabpath": "C:\\\\Program Files\\\\MATLAB\\\\R2019a\\\\bin\\\\matlab.exe",

    "latex-workshop.view.pdf.viewer": "tab",
    "latex-workshop.view.pdf.trim": 1,
    "latex-workshop.latex.autoBuild.cleanAndRetry.enabled": false,
    "spellright.notificationClass": "warning",

    "editor.suggestSelection": "recentlyUsed",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
}